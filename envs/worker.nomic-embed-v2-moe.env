# NATS Configuration
NATS_URL=nats://127.0.0.1:5700
STREAM_NAME=EMBED_NOMIC_V2_MOE
SUBJECT=embedding.request.nomic-embed-v2-moe
QUEUE_DURABLE=nomic-embed-v2-moe-wq
QUEUE_GROUP=workers
RESPONSE_PREFIX=embedding.reply
QUEUE_MAX_MSGS=2000
QUEUE_MAX_AGE=30s
ACK_WAIT=30s
MAX_DELIVER=5
MAX_ACK_PENDING=64

# Worker Configuration
WORKER_CONCURRENCY=2

# HTTP Configuration
HTTP_ADDR=:5779

# Model Configuration
MODEL_NAME=nomic-embed-v2-moe
MODEL_URL=https://huggingface.co/nomic-ai/nomic-embed-text-v2-moe-GGUF/resolve/main/nomic-embed-text-v2-moe.f16.gguf
MODEL_PATH=data/models/nomic-embed-v2-moe/model.gguf
MODEL_FORMAT=embedding
MODEL_THREADS=8
CTX_SIZE=8192

# Data Directory Configuration  
DATA_DIR=data

# Database Configuration
DB_PATH=data/logs/nomic-embed-v2-moe.sqlite

# Monitoring Configuration
MONITORING_TOPIC=monitoring.embedding
BACKPRESSURE_THRESHOLD=3